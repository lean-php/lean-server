{
    "name": "lean/server",
    "description": "Lean Web App Server",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Micha Alt",
            "email": "php@lean-stack.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "Lean\\": "src/",
            "App\\": "app/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lean\\Tests\\": "tests/",
            "App\\Tests\\": "app/tests/"
        }
    },
    "require": {
        "php-di/php-di": "^6.0",
        "league/plates": "^3.3",
        "nikic/fast-route": "^1.3",
        "monolog/monolog": "^1.23"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3"
    },
    "config": {
        "bin-dir": "bin"
    }
}
